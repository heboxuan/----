<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.he.dao.front.FrontCityDao">
    <select id="totalMessageOne" resultType="map">
        SELECT
            id id,
            NAME NAME,
            ask_num askNum,
            response_num reNum
        FROM
            front_city
        where
            id=1
    </select>
</mapper>